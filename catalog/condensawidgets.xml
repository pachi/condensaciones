<?xml version="1.0" encoding="UTF-8"?>
<glade-catalog name="condensaciones" supports="gtkbuilder" depends="gtk+" language="python">
    <glade-widget-classes>
        <glade-widget-class name="CCabecera" generic-name="ccabecera" title="Cabecera condensaciones" parent="GtkEventBox">
        </glade-widget-class>
        <glade-widget-class name="CPTCanvas" generic-name="cptcanvas" title="Plot Canvas" parent="GtkDrawingArea">
            <properties>
                <property id="is-frobnicator" name="Is Frobnicator" ignore="True">
                    <parameter-spec><type>GParamBoolean</type></parameter-spec>
                    <tooltip>Whether this bar is a canvas frobnicator</tooltip>
                </property>
            </properties>
            <signals>
                <signal id="frobnicate" since="1.0"/>
            </signals>
        </glade-widget-class>
        <glade-widget-class name="CPCanvas" generic-name="cpcanvas" title="Plot Canvas" parent="GtkDrawingArea">
            <properties>
                <property id="is-frobnicator" name="Is Frobnicator" ignore="True">
                    <parameter-spec><type>GParamBoolean</type></parameter-spec>
                    <tooltip>Whether this bar is a canvas frobnicator</tooltip>
                </property>
            </properties>
            <signals>
                <signal id="frobnicate" since="1.0"/>
            </signals>
        </glade-widget-class>
        <glade-widget-class name="CPie" generic-name="cpie" title="Pie condensaciones" parent="GtkEventBox">
        </glade-widget-class>
    </glade-widget-classes>
    <glade-widget-group name="condensaciones" title="widgets para condensaciones">
        <glade-widget-class-ref name="CCabecera"/>
        <glade-widget-class-ref name="CPTCanvas"/>
        <glade-widget-class-ref name="CPCanvas"/>
        <glade-widget-class-ref name="CPie"/>
    </glade-widget-group>
</glade-catalog>
